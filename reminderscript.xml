<!DOCTYPE HTML>
<html lang="en">
<body>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script>
	$('#reminder > div:gt(0)').hide();
	
	setInterval(function() { 
	  $('#reminder > div:first')
	    .fadeOut(1000)
	    .next()
	    .fadeIn(1000)
	    .end()
	    .appendTo('#reminder');
	},  3000);
</script>
<style>
	body {
    	background-color: #FFFFFF;
	}
	.hidden { display: none; }
</style>
<div id="reminder" style="position: relative; overflow:hidden;background-color: #FFFFFF; width: 220px;  height: 110px;">
