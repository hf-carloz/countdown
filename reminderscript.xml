<!DOCTYPE HTML>
<html lang="en">
<body>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script>
	$('#reminder > iframe:gt(0)').hide();
	
	setInterval(function() { 
	  $('#reminder > iframe:first')
	    .fadeOut(1000)
	    .next()
	    .fadeIn(1000)
	    .end()
	    .appendTo('#reminder');
	},  3000);
</script>
<div id="reminder" style="position: relative; overflow:hidden;background-color: #FFFFFF; width: 220px;  height: 110px;">
     <iframe src="https://goo.gl/cQ13S7" name="frame" id="frame" width="180px" frameborder="0" scrolling="no"></iframe>
